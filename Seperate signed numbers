;Ten 8-bit signed numbers are stored in memory locations beginning from 2000H. 
;Move all negative numbers in memory locations beginning from 2100H.

JMP START 
;STORE NUMBERS;
START:  MVI A,00
	STA 2000

	MVI A,0F0H;FILL IN SOME NEGETIVES; 11110000
	STA 2001
	STA 2003
	STA 2006
	STA 2009

	MVI A,08H;FILL IN SOME POSITIVES; 00001000
	STA 2002
	STA 2004
	STA 2005
	STA 2007
	STA 2008

	;MAIN PROGRAM;

	LXI H,2000; POINT TO FIRST NUMBER
	LXI D,2100; POINT TO STORAGE
	MVI B,10; COUNTER
	MVI C,0; CLEAR C

LOOP:   MOV A,M; 
	MOV C,M
	ANI 80H; ADD FILTER
	JZ NOTNEG
	XCHG; INTERCHANGE DE AND HL
	MOV M,C; STORE VALUE
	INX H; INCREMENT STORAGE
	XCHG; EXCHANGE AGAIN

NOTNEG:  INX H
	DCR B
	JNZ LOOP
	HLT